#include <iostream>
#include <stdlib.h>
using namespace std;
  
int main(){
    // B for background Color(Light Aqua)
    // 5 for text color(Purple)
    system("Color B5");
    cout << "Geeks";
  
    // 1 for background Color(Blue)
    // 6 for text color(Yellow)
    system("Color 16");
    cout << " For ";
  
    // D for background Color(Light Purple)
    // E for text color(Light Yellow)
    system("Color DE");
    cout << "Geeks";
  
    return 0;
}